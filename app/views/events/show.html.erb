<p>
  <strong>Title:</strong>
  <%= @event.event_title %>
</p>

<p>
  <strong>Location:</strong>
  <%= @event.location %>
</p>

<p>
  <strong>Date:</strong>
  <%= @event.date %>
</p>

 <p>
  <strong>Description:</strong>
  <%= @event.description %>
</p>

<table>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Company</th>
    <th>Title</th>
    
  </tr>

  <% @users.each do |user| %>
    <tr>
      <td><%= user.first_name %></td>
      <td><%= user.last_name %></td>
      <td><%= user.company_name %></td>
      <td><%= user.job_title %></td>
      
    </tr>
  <% end %>
</table>



<%= link_to 'Event Listing', events_path %>
 <%= link_to 'Edit', edit_event_path(@event) %>